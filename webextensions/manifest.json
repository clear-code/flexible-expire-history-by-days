{
  "manifest_version": 2,
  "name": "Expire History By Days",
  "version": "1.3.1",
  "description": "Allows to expire history older than a fixed amount of days.",

  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },

  "permissions": [ "idle", "history", "storage" ],

  "background": {
    "scripts": [ "background.js" ]
  },
  "options_ui": {
    "browser_style": true,
    "page": "options.html"
  }
}
